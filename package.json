{
  "name": "oceanic-builders",
  "version": "0.0.20-alpha",
  "description": "Oceanic compatible utilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "./esm.mjs",
  "exports": {
    ".": {
      "bun": "./dist/index.js",
      "import": "./esm.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
