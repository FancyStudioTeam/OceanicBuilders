{
  "dependencies": {
    "oceanic.js": "1.12.0"
  },
  "description": "Unofficial utilities for Oceanic.js",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@swc/cli": "0.7.3",
    "@swc/core": "1.11.22",
    "@types/node": "22.15.3",
    "husky": "9.1.7",
    "lint-staged": "15.5.1",
    "rimraf": "6.0.1",
    "tsc-alias": "1.8.15",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.2"
  },
  "files": ["dist"],
  "keywords": ["bot", "builders", "discord", "discord bot", "oceanic", "oceanic.js", "typescript"],
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,ts,json,md}": ["pnpm biome"]
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "name": "oceanic-builders",
  "packageManager": "pnpm@9.15.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/FancyStudioTeam/OceanicBuilders"
  },
  "scripts": {
    "biome": "biome check --write --config-path biome.json",
    "build": "pnpm rimraf && pnpm build:esm && pnpm build:cjs && pnpm build:types",
    "build:cjs": "swc src --strip-leading-paths -C module.type=commonjs --out-dir dist/cjs",
    "build:esm": "swc src --strip-leading-paths -C module.type=es6 --out-dir dist/esm",
    "build:types": "tsc --project tsconfig.json --declaration --emitDeclarationOnly --outDir dist/types",
    "postbuild": "tsc-alias -p tsconfig.prod.json dist",
    "prepare": "husky",
    "release": "bash release.sh",
    "rimraf": "rimraf dist",
    "test": "vitest",
    "tgz": "pnpm build && pnpm pack"
  },
  "types": "dist/index.d.ts",
  "version": "1.2.0"
}
