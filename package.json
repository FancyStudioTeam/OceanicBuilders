{
  "author": "FancyStudio",
  "dependencies": {
    "oceanic.js": "1.11.2"
  },
  "description": "A set of utilities for developing using Oceanic.js",
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@types/node": "22.7.4",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "tsx": "4.19.1",
    "typescript": "5.6.2"
  },
  "files": ["dist", "esm.mjs"],
  "homepage": "https://discord.gg/gud55BjNFC",
  "keywords": ["bot", "builders", "discord bot", "discord", "oceanic", "typescript"],
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": ["pnpm biome"]
  },
  "main": "dist/index.js",
  "module": "./esm.mjs",
  "name": "oceanic-builders",
  "packageManager": "pnpm@9.12.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/FancyStudioTeam/OceanicBuilders"
  },
  "scripts": {
    "biome": "biome check --write --config-path=\"./biome.json\" .",
    "build": "tsc",
    "docs:build": "pnpm --filter docs build",
    "docs:dev": "pnpm --filter docs dev",
    "docs:preview": "pnpm --filter docs preview",
    "example": "tsx --watch --env-file=\".env\" example.ts",
    "prepare": "husky",
    "release": "bash release.sh"
  },
  "types": "dist/index.d.ts",
  "version": "1.0.2"
}
