    "oceanic.js": "1.11.1-dev.05899ff",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/node": "20.14.9",
    "tsx": "4.16.0",
    "typescript": "5.5.2",
    "vitest": "1.6.0"
  },
  "packageManager": "pnpm@9.4.0+sha256.b6fd0bfda555e7e584ad7e56b30c68b01d5a04f9ee93989f4b93ca8473c49c74"
